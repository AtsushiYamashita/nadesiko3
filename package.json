{
  "name": "nadesiko3",
  "version": "0.0.4",
  "description": "Japanese Programming Language",
  "main": "nako3",
  "scripts": {
    "test": "mocha",
    "start": "node server.js",
    "build": "webpack",
    "build:parser": "pegjs --cache -o src/nako_parser.js src/nako_parser.pegjs",
    "build:command": "node src/cnako3.js batch/build_command.nako3",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kujirahand/nadesiko3.git"
  },
  "keywords": [
    "script"
  ],
  "author": "kujirahand",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kujirahand/nadesiko3/issues"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 1 version",
              "last 2 Safari versions",
              "last 2 iOS versions"
            ]
          }
        }
      ],
      "react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "env": {
      "mocha": true,
      "browser": true
    },
    "rules": {
      "no-eval": "off",
      "react/jsx-uses-vars": 1
    }
  },
  "homepage": "https://github.com/kujirahand/nadesiko3#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.2.0",
    "babel-preset-react": "^6.23.0",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.2.0",
    "pegjs": "^0.10.0",
    "pump": "^1.0.2",
    "source-map": "^0.5.6",
    "webpack": "^2.3.0"
  },
  "dependencies": {
    "express": "^4.15.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "serve-index": "^1.8.0",
    "superagent": "^3.5.1"
  }
}
